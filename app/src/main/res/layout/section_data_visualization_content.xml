<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <android.support.constraint.ConstraintLayout
        android:id="@+id/cont_visualization_content"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_weight="8.25">

        <Space
            android:id="@+id/space_overview_start"
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/cont_scroll_graph"
            app:layout_constraintHorizontal_weight="0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <Space
            android:id="@+id/space_overview_end"
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="@+id/graph_container_center"
            app:layout_constraintHorizontal_weight="4.5"
            app:layout_constraintStart_toEndOf="@+id/cont_scroll_graph"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.0" />

        <LinearLayout
            android:id="@+id/cont_right_items"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:orientation="vertical"
            android:weightSum="1"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/cont_scroll_graph"
            app:layout_constraintTop_toTopOf="parent">

            <android.support.constraint.ConstraintLayout
                android:id="@+id/cont_right_items_top"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_marginStart="20dp"
                android:layout_marginEnd="20dp"
                android:layout_weight="0.175"
                android:background="@color/lightGrey">

                <ImageView
                    android:id="@+id/imageView9"
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    android:layout_marginBottom="1dp"
                    android:contentDescription="@string/cd_blank"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:srcCompat="@color/white" />

                <Space
                    android:id="@+id/space_overview_graph_left2"
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toStartOf="@id/cont_patient_details"
                    app:layout_constraintHorizontal_weight="0"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <Space
                    android:id="@+id/space_overview_graph_right2"
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_weight="0"
                    app:layout_constraintStart_toEndOf="@+id/cont_patient_details"
                    app:layout_constraintTop_toTopOf="parent" />

                <android.support.constraint.ConstraintLayout
                    android:id="@+id/constraintLayout3"
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    android:layout_marginTop="5dp"
                    android:background="@color/lightGrey"
                    app:layout_constraintBottom_toTopOf="@+id/cont_patient_details"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintVertical_weight="6.5">

                    <ImageView
                        android:id="@+id/imageView8"
                        android:layout_width="0dp"
                        android:layout_height="0dp"
                        android:layout_marginBottom="1dp"
                        android:contentDescription="@string/cd_blank"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        app:srcCompat="@color/white" />

                    <TextView
                        android:id="@+id/tv_name_r"
                        style="@style/TitleText"
                        android:layout_width="0dp"
                        android:layout_height="0dp"
                        android:layout_marginStart="10dp"
                        android:layout_marginEnd="10dp"
                        android:gravity="center"
                        android:includeFontPadding="false"
                        android:padding="5dp"
                        android:text="@string/overview_title"
                        android:textAlignment="center"
                        android:textAppearance="@style/DefaultText"
                        android:textColor="@color/greenHighlight"
                        app:autoSizePresetSizes="@array/autosize_text_sizes"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_weight="8"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />
                </android.support.constraint.ConstraintLayout>

                <android.support.constraint.ConstraintLayout
                    android:id="@+id/cont_patient_details"
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    android:layout_marginBottom="1dp"
                    android:background="@color/lightGrey"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toStartOf="@+id/space_overview_graph_right2"
                    app:layout_constraintHorizontal_weight="10"
                    app:layout_constraintStart_toEndOf="@+id/space_overview_graph_left2"
                    app:layout_constraintTop_toBottomOf="@+id/constraintLayout3"
                    app:layout_constraintVertical_weight="3.5">

                    <android.support.constraint.ConstraintLayout
                        android:id="@+id/cont_highlight_2"
                        android:layout_width="0dp"
                        android:layout_height="0dp"
                        android:background="@color/white"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_weight="8.5"
                        app:layout_constraintStart_toEndOf="@+id/cont_highlight_1"
                        app:layout_constraintTop_toTopOf="parent">

                        <ImageView
                            android:id="@+id/imageView12"
                            android:layout_width="1dp"
                            android:layout_height="0dp"
                            android:contentDescription="@string/cd_blank"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintHorizontal_bias="1.0"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent"
                            app:srcCompat="@color/lightGrey" />

                        <TextView
                            android:id="@+id/textView14"
                            android:layout_width="0dp"
                            android:layout_height="0dp"
                            android:layout_marginStart="10dp"
                            android:layout_marginTop="2dp"
                            android:layout_marginEnd="10dp"
                            android:layout_marginBottom="2dp"
                            android:gravity="center_vertical|start"
                            android:text="@string/highlight_highest"
                            app:autoSizePresetSizes="@array/autosize_text_sizes"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent" />

                        <Space
                            android:id="@+id/space6"
                            android:layout_width="0dp"
                            android:layout_height="0dp"
                            android:visibility="invisible"
                            app:layout_constraintBottom_toTopOf="@+id/cont_bmi_image"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent"
                            app:layout_constraintVertical_weight="3" />

                        <android.support.constraint.ConstraintLayout
                            android:id="@+id/cont_bmi_image"
                            android:layout_width="0dp"
                            android:layout_height="0dp"
                            android:visibility="invisible"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent"
                            app:layout_constraintVertical_weight="9">

                            <Space
                                android:id="@+id/spacebottom"
                                android:layout_width="0dp"
                                android:layout_height="0dp"
                                app:layout_constraintBottom_toBottomOf="parent"
                                app:layout_constraintEnd_toEndOf="parent"
                                app:layout_constraintStart_toStartOf="parent"
                                app:layout_constraintTop_toBottomOf="@+id/img_patient_bmi"
                                app:layout_constraintVertical_weight="3.5" />

                            <Space
                                android:id="@+id/spacetop"
                                android:layout_width="0dp"
                                android:layout_height="0dp"
                                app:layout_constraintBottom_toTopOf="@+id/tv_bmi_title"
                                app:layout_constraintEnd_toEndOf="parent"
                                app:layout_constraintHorizontal_bias="0.0"
                                app:layout_constraintStart_toStartOf="parent"
                                app:layout_constraintTop_toTopOf="parent"
                                app:layout_constraintVertical_weight="6" />

                            <TextView
                                android:id="@+id/tv_bmi_title"
                                style="@style/SmallContextText"
                                android:layout_width="0dp"
                                android:layout_height="0dp"
                                android:layout_weight="1"
                                android:elevation="105dp"
                                android:gravity="center_vertical|center_horizontal"
                                android:padding="0dp"
                                android:text="@string/bmi"
                                android:textAlignment="center"
                                app:autoSizePresetSizes="@array/autosize_text_sizes"
                                app:layout_constraintBottom_toBottomOf="parent"
                                app:layout_constraintEnd_toEndOf="parent"
                                app:layout_constraintStart_toStartOf="parent"
                                app:layout_constraintTop_toBottomOf="@+id/spacetop"
                                app:layout_constraintVertical_weight="4" />

                            <ImageView
                                android:id="@+id/img_patient_bmi_color"
                                android:layout_width="0dp"
                                android:layout_height="0dp"
                                android:layout_marginTop="3dp"
                                android:contentDescription="@string/cd_blank"
                                app:layout_constraintBottom_toTopOf="@+id/spacebottom"
                                app:layout_constraintEnd_toEndOf="parent"
                                app:layout_constraintStart_toStartOf="parent"
                                app:layout_constraintTop_toTopOf="parent"
                                app:layout_constraintVertical_weight="6.5"
                                app:srcCompat="@drawable/img_heart_overlay_normal" />

                            <ImageView
                                android:id="@+id/img_patient_bmi"
                                android:layout_width="0dp"
                                android:layout_height="0dp"
                                android:layout_marginTop="3dp"
                                android:clickable="true"
                                android:contentDescription="@string/cd_blank"
                                android:elevation="100dp"
                                android:focusable="true"
                                app:layout_constraintBottom_toTopOf="@+id/spacebottom"
                                app:layout_constraintEnd_toEndOf="parent"
                                app:layout_constraintStart_toStartOf="@+id/spacetop"
                                app:layout_constraintTop_toTopOf="parent"
                                app:layout_constraintVertical_weight="6.5"
                                app:srcCompat="@drawable/img_heart_overlay" />

                            <TextView
                                android:id="@+id/tv_bmi"
                                style="@style/SmallContextText"
                                android:layout_width="0dp"
                                android:layout_height="0dp"
                                android:layout_marginTop="2dp"
                                android:layout_marginBottom="2dp"
                                android:elevation="105dp"
                                android:gravity="center"
                                android:padding="0dp"
                                android:text="@string/bmi"
                                android:textAlignment="gravity"
                                android:textColor="@color/colorPrimaryDark"
                                android:visibility="invisible"
                                app:autoSizePresetSizes="@array/autosize_text_sizes"
                                app:layout_constraintBottom_toTopOf="@+id/tv_bmi_title"
                                app:layout_constraintEnd_toEndOf="parent"
                                app:layout_constraintStart_toStartOf="parent"
                                app:layout_constraintTop_toTopOf="@+id/img_patient_bmi"
                                app:layout_constraintVertical_weight="5.5" />
                        </android.support.constraint.ConstraintLayout>

                        <Space
                            android:id="@+id/space5"
                            android:layout_width="0dp"
                            android:layout_height="0dp"
                            android:layout_marginBottom="-1dp"
                            android:visibility="invisible"
                            app:layout_constraintBottom_toTopOf="@+id/cont_bmi_image"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent"
                            app:layout_constraintVertical_weight="1.5" />

                    </android.support.constraint.ConstraintLayout>

                    <android.support.constraint.ConstraintLayout
                        android:id="@+id/cont_highlight_1"
                        android:layout_width="0dp"
                        android:layout_height="0dp"
                        android:background="@color/white"
                        android:visibility="visible"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toStartOf="@+id/cont_highlight_2"
                        app:layout_constraintHorizontal_weight="1.5"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent">

                        <ImageView
                            android:id="@+id/imageView11"
                            android:layout_width="0dp"
                            android:layout_height="0dp"
                            android:adjustViewBounds="false"
                            android:background="@color/theme_menu_button"
                            android:contentDescription="@string/cd_blank"
                            android:cropToPadding="false"
                            app:layout_constraintBottom_toBottomOf="@+id/imageView10"
                            app:layout_constraintEnd_toEndOf="@+id/imageView10"
                            app:layout_constraintStart_toStartOf="@+id/imageView10"
                            app:layout_constraintTop_toTopOf="@+id/imageView10" />

                        <ImageView
                            android:id="@+id/imageView10"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:contentDescription="@string/cd_blank"
                            android:cropToPadding="false"
                            android:visibility="invisible"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent"
                            app:srcCompat="@drawable/img_sidebar_visualize" />

                        <TextView
                            android:id="@+id/tv_height"
                            style="@style/SmallLabelText"
                            android:layout_width="0dp"
                            android:layout_height="0dp"
                            android:layout_marginStart="5dp"
                            android:layout_marginTop="5dp"


                            android:layout_marginEnd="5dp"
                            android:layout_weight="1"
                            android:gravity="bottom"
                            android:padding="0dp"
                            android:text="@string/height"
                            android:textAlignment="center"
                            android:textColor="@color/colorPrimaryDark"
                            android:visibility="invisible"
                            app:autoSizePresetSizes="@array/autosize_text_sizes"
                            app:layout_constraintBottom_toTopOf="@+id/tv_height_label"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent"
                            app:layout_constraintVertical_weight="6" />

                        <TextView
                            android:id="@+id/tv_height_label"
                            style="@style/SmallLabelText"
                            android:layout_width="0dp"
                            android:layout_height="0dp"
                            android:layout_weight="1"
                            android:gravity="top"
                            android:padding="0dp"
                            android:text="@string/height"
                            android:textAlignment="center"
                            android:visibility="invisible"
                            app:autoSizePresetSizes="@array/autosize_text_sizes"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toBottomOf="@+id/tv_height"
                            app:layout_constraintVertical_weight="4" />
                    </android.support.constraint.ConstraintLayout>

                </android.support.constraint.ConstraintLayout>
            </android.support.constraint.ConstraintLayout>

            <android.support.constraint.ConstraintLayout
                android:id="@+id/cont_graph_overview_spacing"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="0.8">

                <Space
                    android:id="@+id/space_overview_graph_left"
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toStartOf="@id/include_stacked_graph_overview"
                    app:layout_constraintHorizontal_weight="0.005"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <include
                    android:id="@+id/include_stacked_graph_overview"
                    layout="@layout/scroll_visualization_stacked_graph"
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="@id/space_overview_graph_right"
                    app:layout_constraintHorizontal_weight="0.99"
                    app:layout_constraintStart_toEndOf="@id/space_overview_graph_left"
                    app:layout_constraintTop_toTopOf="parent" />

                <Space
                    android:id="@+id/space_overview_graph_right"
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_weight="0.005"
                    app:layout_constraintStart_toEndOf="@+id/include_stacked_graph_overview"
                    app:layout_constraintTop_toTopOf="parent" />

            </android.support.constraint.ConstraintLayout>

            <android.support.constraint.ConstraintLayout
                android:id="@+id/cont_right_items_bottom"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="0.05">

            </android.support.constraint.ConstraintLayout>

        </LinearLayout>

        <android.support.constraint.ConstraintLayout
            android:id="@+id/cont_scroll_graph"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:background="@color/lightGrey"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/space_overview_end"
            app:layout_constraintHorizontal_weight="5"
            app:layout_constraintStart_toEndOf="@+id/space_overview_start"
            app:layout_constraintTop_toTopOf="parent">

            <ImageView
                android:id="@+id/imageView6"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:layout_marginEnd="1dp"
                android:contentDescription="@string/cd_blank"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:srcCompat="@color/white" />

            <Space
                android:id="@+id/spc_top_filler"
                android:layout_width="0dp"
                android:layout_height="0dp"
                app:layout_constraintBottom_toTopOf="@id/include_school_header"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_weight="0.25" />

            <Space
                android:id="@+id/spc_left_filler"
                android:layout_width="0dp"
                android:layout_height="0dp"
                app:layout_constraintBottom_toBottomOf="@+id/include_school_header"
                app:layout_constraintEnd_toStartOf="@+id/include_school_header"
                app:layout_constraintHorizontal_weight="0.5"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <Space
                android:id="@+id/spc_right_filler"
                android:layout_width="0dp"
                android:layout_height="0dp"
                app:layout_constraintBottom_toBottomOf="@+id/include_school_header"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_weight="0.5"
                app:layout_constraintStart_toEndOf="@+id/include_school_header"
                app:layout_constraintTop_toTopOf="parent" />

            <include
                android:id="@+id/include_school_header"
                layout="@layout/section_data_visualization_header"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:layout_marginStart="10dp"
                android:layout_marginEnd="10dp"
                android:layout_marginBottom="8dp"
                app:layout_constraintBottom_toTopOf="@+id/cont_overview_avatar"
                app:layout_constraintEnd_toStartOf="@id/spc_right_filler"
                app:layout_constraintHorizontal_weight="9"
                app:layout_constraintStart_toEndOf="@id/spc_left_filler"
                app:layout_constraintTop_toBottomOf="@id/spc_top_filler"
                app:layout_constraintVertical_weight="2" />

            <android.support.constraint.ConstraintLayout
                android:id="@+id/cont_overview_avatar"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:visibility="visible"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/include_school_header"
                app:layout_constraintVertical_weight="7.75">

                <android.support.constraint.ConstraintLayout
                    android:id="@+id/cont_specific_top"
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    app:layout_constraintBottom_toTopOf="@+id/cont_specific_bottom"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintVertical_weight="8">

                    <Space
                        android:id="@+id/space_specific_circle_top"
                        android:layout_width="0dp"
                        android:layout_height="0dp"
                        app:layout_constraintBottom_toTopOf="@+id/cont_specific_chart"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintVertical_weight="0.25" />

                    <LinearLayout
                        android:id="@+id/cont_specific_chart"
                        android:layout_width="0dp"
                        android:layout_height="0dp"
                        android:orientation="horizontal"
                        android:weightSum="1"
                        app:layout_constraintBottom_toTopOf="@+id/space_specific_circle_bottom"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/space_specific_circle_top"
                        app:layout_constraintVertical_weight="8.75">

                        <Space
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="0.05" />

                        <RelativeLayout
                            android:id="@+id/graph_specific_bar_single"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="0.9">

                        </RelativeLayout>

                        <Space
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="0.05" />

                    </LinearLayout>

                    <Space
                        android:id="@+id/space_specific_circle_bottom"
                        android:layout_width="0dp"
                        android:layout_height="0dp"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/cont_specific_chart"
                        app:layout_constraintVertical_weight="1" />

                    <TextView
                        android:id="@+id/tv_specific_title"
                        android:layout_width="0dp"
                        android:layout_height="0dp"
                        android:gravity="center"
                        android:includeFontPadding="false"
                        android:text="@string/cd_blank"
                        android:textColor="@color/colorPrimaryDark"
                        android:textStyle="bold"
                        app:autoSizePresetSizes="@array/autosize_text_sizes"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/cont_specific_chart" />

                </android.support.constraint.ConstraintLayout>

                <android.support.constraint.ConstraintLayout
                    android:id="@+id/cont_specific_bottom"
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/cont_specific_top"
                    app:layout_constraintVertical_weight="2">

                </android.support.constraint.ConstraintLayout>

            </android.support.constraint.ConstraintLayout>

        </android.support.constraint.ConstraintLayout>

        <RelativeLayout
            android:id="@+id/graph_container_center"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:visibility="visible"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_weight="6">

        </RelativeLayout>

        <LinearLayout
            android:id="@+id/cont_graph_container"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:baselineAligned="false"
            android:orientation="horizontal"
            android:weightSum="1"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_weight="6"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <RelativeLayout
                android:id="@+id/graph_container_left"

            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/graph_container_right"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="0.5"
                android:clipToPadding="false"
                android:gravity="top">

            </RelativeLayout>

        </LinearLayout>

    </android.support.constraint.ConstraintLayout>
</android.support.constraint.ConstraintLayout>